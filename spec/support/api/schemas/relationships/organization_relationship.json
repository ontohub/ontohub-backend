{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Organization Relationship",
  "description": "Organization Relationship",

  "type": "object",
  "required": ["id", "type"],
  "properties": {
    "id": {
      "type": "string",
      "description": "The id of the organization",
      "example": "seed-users"
    },
    "type": {
      "type": "string",
      "enum": ["organizations"],
      "description": "The type of the organization. This is always \"organizations\".",
      "example": "organizations"
    }
  }
}

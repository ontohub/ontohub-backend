{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "User Relationship",
  "description": "User Relationship",

  "type": "object",
  "required": ["id", "type"],
  "properties": {
    "id": {
      "type": "string",
      "description": "The id of the user",
      "example": "ada"
    },
    "type": {
      "type": "string",
      "enum": ["users"],
      "description": "The type of the user. This is always \"users\".",
      "example": "users"
    }
  }
}
